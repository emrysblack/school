#include <iostream>
#include <cassert>
#include "skeet.h"
using namespace std;

/*******************************************************
 * defualt constructor for skeet
 *******************************************************/
Skeet :: Skeet()
{
   cout << "Bird regenerate position: ";
   cin >> pos;
   cout << "Bird regenerate horizontal velocity: ";
   cin >> hVol;
   cout << "Bird regenerate vertical velocity / 5: ";
   cin >> vVol;
   isDead = false;
}

/*******************************************************
 * deconstructor for skeet
 *******************************************************/
Skeet :: ~Skeet()
{
   isDead = true;
}
